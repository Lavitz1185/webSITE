                        <a class="blue" id="login" asp-area="Login" asp-controller="Account" asp-area="">LOGIN MAHASISWA</a>
                    </div>
                }
            </div>
        </div>

        <!--dots-->
        <ul class="dots">
            <li class="active"></li>
            <li></li>
            <li></li>
        </ul>

    </div>
</section>
