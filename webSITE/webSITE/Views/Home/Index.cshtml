@section Scripts{
    <partial name="_TransparentNavbar" />
    <partial name="_OwlCarouselPartial" />


    <script type="text/javascript">
        var owl = $('.owl-carousel');
        owl.owlCarousel({
            loop: false,
            nav: true,
            margin: 5,
            responsive: {
                0: {
                    items: 1
                },
                600: {
                    items: 3
                },
                1500: {
                    items: 4
                },
                1920: {
                    items: 5
                },
            }
        });


        $(document).ready(function () {
            const slider = $(".slider");
            const items = $(".item");
            const prevBtn = $("#prev");
            const nextBtn = $("#next");
            const dots = $(".dots li");

            let index = 0;
            let interval;

            function showSlide() {
                $(items).each((i, item) => {
                    if (index === i) {
                        $(item).css({
                            "opacity": "1",
                            "pointerEvents": "auto"
                        });
                    } else {
                        $(item).css({
                            "opacity": "0",
                            "pointerEvents": "none"
                        });
                    }
                });

                updateDots();
            }

            function updateDots() {
                $(dots).each((i, item) => {
                    if (i === index) {
                        $(item).addClass('active');
                    }
                    else {
                        $(item).removeClass('active');
                    }
                });
            }

            function nextSlide() {
                index = (index + 1) % items.length;
                showSlide();
            }

            function prevSlide() {
                index = (index - 1 + items.length) % items.length;
                showSlide();
            }

            function startAutoSlide() {
                interval = setInterval(nextSlide, 5000); // slide setiap 5s
            }

            function stopAutoSlide() {
                clearInterval(interval);
            }

            function checkScreenWidth() {
                if (window.innerWidth < 768) {
                    prevBtn.addClass('transparent');
                    nextBtn.addClass('transparent');
                }
                else {
                    prevBtn.removeClass('transparent');
                    nextBtn.removeClass('transparent');
                }
            }

            $(nextBtn).on("click", () => {
                nextSlide();
                stopAutoSlide();
            });

            $(prevBtn).on("click", () => {
                prevSlide();
                stopAutoSlide();
            });

            $(dots).each((i, item) => {
                $(item).on('click', () => {
                    index = i;
                    showSlide();
                    stopAutoSlide();
                });
            });

            $(slider).on("mouseenter", stopAutoSlide);
            $(slider).on("mouseleave", startAutoSlide);

            checkScreenWidth();
            startAutoSlide();
        });


