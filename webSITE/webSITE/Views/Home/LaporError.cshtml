        <!-- Judul -->
        <h4>Laporkan Error</h4>
        <!-- Deskripsi Singkat -->
        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
        <!-- Input -->
        <div class="laporkan-row">
            <input type="text" placeholder="Nama" />
